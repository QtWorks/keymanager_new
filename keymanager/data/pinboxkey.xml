<?xml version="1.0" encoding="UTF-8"?>
<block name="PIN BOX">
        <block name="MAKING A PIN BOX WITH SLIDER" set_variable="qt_type_of_key_qt" level="1" keypreviewimage=":/images/pin_box.png" value="pin_box">
