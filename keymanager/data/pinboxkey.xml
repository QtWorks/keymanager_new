                <parameter name="PIN HOLE WIDTH" type="double" variable="qt_pin_box_hole_width_qt" ui="lineedit" />
                <parameter name="PIN HOLE LENGTH" type="double" variable="qt_pin_box_pin_length_qt" ui="lineedit" />
